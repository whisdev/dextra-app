    "vercel-build": "cross-env NODE_OPTIONS=--max_old_space_size=2048 pnpm npx prisma generate && next build",
    "dev": "cross-env NODE_NO_WARNINGS=1 next dev --turbopack --port 8080",
    "build": "next build",
    "start": "cross-env PORT=8080 next start",
